{
  "name": "react-skeleton",
  "version": "0.1.0",
  "description": "skeleton for web apps with react-js",
  "main": "index.tsx",
  "scripts": {
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "dev": "npm run webpack-dev-server -- --env.mode development --hot",
    "prod": "npm run webpack -- --env.mode production",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev:debug": "npm run debug -- --env.mode development",
    "lint": "eslint 'src/**/*.jsx'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShivamLatawa/react-skeleton.git"
  },
  "keywords": [
    "skeleton",
    "react"
  ],
  "author": "Shivam Latawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShivamLatawa/react-skeleton/issues"
  },
  "homepage": "https://github.com/ShivamLatawa/react-skeleton#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
